<h1 class="text-center">Player Stats</h1>
<table class="table table-striped">
    <thead>
    <tr>
        <th>Attribute</th>
        <th>Value</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td>Health</td>
        <td>${stats.health}</td>
    </tr>
    <tr>
        <td>Strength</td>
        <td>${stats.strength}</td>
    </tr>
    <tr>
        <td>Agility</td>
        <td>${stats.agility}</td>
    </tr>
    <tr>
        <td>Intelligence</td>
        <td>${stats.intelligence}</td>
    </tr>
    </tbody>
</table>
